public abstract class Contrainte {

    private String nom;
    private String valeur;

    public void setNom(String nom) {
        this.nom = nom;
    }

    public void setValeur(String valeur) {
        this.valeur = valeur;
    }

    @Override
    public String toString() {
        return (nom + " : " + valeur);
    }
}
